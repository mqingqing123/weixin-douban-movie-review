page {
  height: 100%;
}
.container {
  height: 100%;
}
.splash {
  height: 100%;
}
.splash swiper-item {
  flex: 1;
}
.splash swiper-item image {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: .9;
}
.start {
  position: absolute;
  bottom: 150rpx;
  left: 50%;
  width: 300rpx;
  margin-left: -150rpx;
  background-color: rgba(63, 188, 64, 0.7);
  color: #fff;
  border: 1rpx solid rgba(63, 188, 64, 0.75);
  font-size: 35rpx;
}
